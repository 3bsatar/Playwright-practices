{
  "name": "playwright-practices",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rimraf allure-report allure-results && mkdirp allure-report allure-results",
    "generate:reports": "npx allure generate ./allure-results --clean",
    "open:report": "npx allure open ./allure-report",
    "pretest": "npm run clean:reports",
    "posttest": "npm run generate:reports && npm run open:report",
    "script:to:run": "npx playwright test pom",
    "test": "npm run script:to:run || npm run posttest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.4.3",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.1.2"
  }
}
